@using RC.CA.SharedKernel.WebHelpers
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rc-nonce rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous"/>
    <link rc-nonce rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.0/css/all.min.css" integrity="sha512-BnbUDfEUfV0Slx6TunuB042k9tuKe3xrD6q4mg5Ed72LTgzDIcLPxg6yI2gcMFRyomt+yJJxE+zJwNmxki6/RA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rc-nonce rel="stylesheet" href='@Url.Content("~/css/rc.site.css")' asp-append-version="true" />
    <link rc-nonce rel="stylesheet" href='@Url.Content("~/css/rc.list.css")' asp-append-version="true" />
    <link rc-nonce rel="stylesheet" href='@Url.Content("~/css/jquery.toast.min.css")' asp-append-version="true" />

    
</head>
<body class="">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Testing / Training / Up skilling</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Club" asp-controller="Member" asp-action="Index">CRUD example</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Cdn" asp-controller="Image" asp-action="Index">Azure image upload</a>
                        </li>
                        @* <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Cdn" asp-controller="CsvFile" asp-action="CsvFileIndex">Csv file upload</a>
                        </li>*@
                         <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="exceptions" asp-controller="csp" asp-action="csptest">CSP test</a>
                        </li>
                       
                    </ul>
                </div>
                <div class="mr-2">
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

<footer id="sticky-footer" class="flex-shrink-0 py-4 bg-white text-black-50">
    <div class="container text-start">
      <small>Richard Coady 2022</small>
    </div>
  </footer>

<script rc-nonce src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script rc-nonce src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script rc-nonce src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script rc-nonce src='@Url.Content("~/js/jquery.filedrop.js")' asp-append-version="true"></script>
<script rc-nonce src='@Url.Content("~/js/jquery.toast.min.js")' asp-append-version="true"></script>

<script rc-nonce src='@Url.Content("~/js/rc.state.js")' asp-append-version="true"></script>
<script rc-nonce src='@Url.Content("~/js/rc.ajaxServices.js")' asp-append-version="true"></script>
<script rc-nonce src='@Url.Content("~/js/rc.formValidation.js")' asp-append-version="true"></script>
<script rc-nonce src='@Url.Content("~/js/rc.page.js")' asp-append-version="true"></script>
<script rc-nonce src='@Url.Content("~/js/rc.list.js")' asp-append-version="true"></script>

@await RenderSectionAsync("Scripts", required: false)

</body>

</html>
